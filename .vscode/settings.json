{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/*.d.ts": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "coverage-gutters.coverageReportFileName": "coverage/**/index.html",
    "coverage-gutters.lcovname": "coverage/lcov.info"
}
